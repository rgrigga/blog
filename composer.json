{
    "name": "rgrigga/blog",
    "description": "a simple blog",
    "keywords":["laravel","blog","example"],
    "authors": [
        {
            "name": "rgrigga",
            "email": "rgrigga@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Rgrigga\\Blog\\": "src/"
        }
    },
    "scripts":{
        "post-update":"php artisan asset:publish rgrigga/blog"
    },
    "minimum-stability": "dev"
}
